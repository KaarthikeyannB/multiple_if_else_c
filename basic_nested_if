#include<stdio.h>
int main(){
   printf("To find the greatest number:\n");
   int a,b;
   scanf("%d%d",&a,&b);
   if(a==b){
   printf("Both are equal");
   }
   else if(a>b){
   printf("%d is greater than %d",a,b);
   }
   else if(b>a){
   printf("%d is greater than %d",b,a);
   }
   else{
   printf("Invalid");
   }
   return 0;
}
